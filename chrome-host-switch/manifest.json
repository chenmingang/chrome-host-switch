{
  "manifest_version": 2,
  "name": "迷途Host",
  "description": "迷途Host",
  "icons": {
    "16"  : "icons/icon_16.png",
    "19"  : "icons/icon_19.png",
    "128" : "icons/icon_128.png"
  },
    "browser_action": {
    "default_icon": "icons/icon_16.png",
    "default_title": "Host Switch",
    "default_popup": "pages/main.html"
  },
  "permissions": [
    "proxy", "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "contextMenus",
    "downloads",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "minimum_chrome_version": "22",
  "version": "2.0"
}
